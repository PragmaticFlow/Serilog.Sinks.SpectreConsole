{
  "Serilog": {
    "MinimumLevel": "Verbose",
    "Using": [ "Serilog.Sinks.SpectreConsole" ],
    "WriteTo": [
      {
        "Name": "SpectreConsole",
        "Args": {
          "outputTemplate": "{Timestamp:HH:mm:ss} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
          "minLevel": "Verbose"
        }
      }
    ]
  }
}
